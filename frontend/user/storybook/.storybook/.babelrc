{
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathSuffix": "../.template_output",
            "rootPathPrefix": "@templates"
          },
          {
            "rootPathSuffix": "./src/utils",
            "rootPathPrefix": "@utils"
          },
		  {
            "rootPathSuffix": "../../_core",
            "rootPathPrefix": "@core"
          }
        ]
      }
    ]
  ]
}