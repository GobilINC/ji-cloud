{
  "private": true,
  "author": "David Komer <david.komer@gmail.com>",
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "build:release": "npm run clean && cross-env RUSTUP_TOOLCHAIN=nightly rollup --config rollup.release.js",
    "build:sandbox": "npm run clean && cross-env RUSTUP_TOOLCHAIN=nightly rollup --config rollup.sandbox.js",
    "dev": "npm run _devfiles && run-p _mediaserver _watch",
    "clean": "rimraf ./dist",
    "_watch": "cross-env RUSTUP_TOOLCHAIN=nightly rollup --config rollup.dev.js --watch",
    "_mediaserver": "cd ../../../build-utils && npm run local-media-server",
    "_devfiles": "cd ../../../build-utils && npm run frontend-dev-files -- user"
  },
  "dependencies": {
    "@wasm-tool/rollup-plugin-rust": "^1.0.3",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-serve": "^1.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "serve-index": "^1.9.1"
  }
}
