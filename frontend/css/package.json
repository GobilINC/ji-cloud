{
  "name": "css",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:storybook": "watchexec -w ./src -w ../.template_output -- postcss src/index.css --env production -o dist/styles.storybook.css",
    "bundle:storybook": "postcss src/index.css --env production -o dist/styles.storybook.css",
    "bundle:all": "npm-run-all -p bundle:prod bundle:debug",
    "bundle:prod": "postcss src/index.css --env production -o dist/styles.min.css",
    "bundle:debug": "postcss src/index.css --env development -o dist/styles.debug.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-url": "^10.1.0",
    "tailwindcss": "^2.0.1"
  }
}
